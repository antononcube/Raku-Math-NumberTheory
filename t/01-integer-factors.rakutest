#!/usr/bin/env raku
use v6.d;

use Test;

use lib <. lib>;
use Math::NumberTheory;

plan *;

## 1
is-deeply factor-integer(11).Array, [(11, 1),];


## 2
is-deeply factor-integer(1).Array, [(1, 1),];

## 3
my $n3 = 30;
is ([*] factor-integer($n3).map({ $_.head ** $_.tail })), $n3;

## 4
is-deeply factor-integer(120).Array, [(2, 3), (3, 1), (5, 1)];

done-testing;